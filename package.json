  "keywords": [
    "web",
    "dataviz",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
