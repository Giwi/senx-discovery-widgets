{
  "name": "@senx/discovery-widgets",
  "version": "1.0.106",
  "description": "Discovery Widgets Elements",
  "keywords": [
    "web",
    "dataviz",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "homepage": "https://warp10.io/",
  "private": false,
  "main": "dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/discovery/discovery.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "dist": "stencil build",
    "build": "npx browserslist@latest --update-db && stencil build --docs --prod",
    "build:dev": "stencil build --docs --dev --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "gen": "stencil generate",
    "storybook.run": "start-storybook -p 6006 -s ./www",
    "storybook": "npm-run-all --parallel build:dev storybook.run",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -fr ./www && rm -fr ./dist",
    "dev": "npm run storybook",
    "build-storybook": "build-storybook -c ./.storybook -o ../public",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'"
  },
  "dependencies": {
    "@stencil/core": "^2.12.1",
    "@stencil/sass": "^2.0.0-0",
    "@tarekraafat/autocomplete.js": "^10.2.6",
    "dayjs": "^1.10.7",
    "dom-to-image": "^2.6.0",
    "echarts": "^5.2.2",
    "echarts-gl": "^2.0.8",
    "element-resize-event": "^3.0.6",
    "fitty": "^2.3.5",
    "flatpickr": "^4.6.9",
    "leaflet": "^1.7.1",
    "leaflet-ant-path": "^1.3.0",
    "leaflet-canvas-layer": "^1.0.8",
    "leaflet-edgebuffer": "^1.0.6",
    "leaflet.heat": "^0.2.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "nouislider": "^15.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "~7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/html": "^6.4.19",
    "@storybook/web-components": "^6.4.19",
    "@types/element-resize-event": "^3.0.0",
    "@types/node": "^16.9.6",
    "babel-loader": "8.2.2",
    "browserslist": "^4.17.2",
    "case": "^1.6.3",
    "lit-html": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "storybook-events-logger": "^2.0.5",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-stencil": "^1.0.1",
    "typescript": "^4.4.3"
  }
}
