{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": false,
    "lib": [
      "dom",
      "es2017",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "es2017",
    "noImplicitAny": false,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [
    "src"
  ],
  "allowJS": true,
  "exclude": [
    "node_modules"
  ]
}
